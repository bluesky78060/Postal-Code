# 우편번호 API 설정
JUSO_API_KEY=your_juso_api_key_here
KAKAO_API_KEY=your_kakao_api_key_here
VWORLD_API_KEY=your_vworld_api_key_here

# 서버 설정
PORT=3001
NODE_ENV=development

# CORS 보안 설정
# 프로덕션: 실제 도메인으로 변경 필요
# 예: ALLOWED_ORIGINS=https://your-app.vercel.app,https://www.yourdomain.com
ALLOWED_ORIGINS=

# 프론트엔드 URL
FRONTEND_URL=http://localhost:3001

# 우편번호 제공자 (local|juso|kakao|koreapost|vworld|open)
POSTAL_PROVIDER=juso

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# 파일 업로드
MAX_FILE_SIZE=10485760
MAX_ROWS=300
